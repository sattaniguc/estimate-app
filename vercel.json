{
  "version": 2,
  "builds": [
    {
      "src": "estimate-app-updated.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/estimate-app-updated.html"
    }
  ],
  "env": {
    "NOTION_TOKEN": "@notion-token"
  }
}
